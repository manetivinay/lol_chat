<h1>Users</h1>
<table class="table table-striped">
  <thead>
  <tr>
    <th data-field="id">Name</th>
    <th data-field="name">Email</th>
    <th data-field="price">Created At</th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= time_ago_in_words user.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>